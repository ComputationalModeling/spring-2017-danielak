@article{star_institutional_1989,
  title = {Institutional {{Ecology}}, '{{Translations}}' and {{Boundary Objects}}: {{Amateurs}} and {{Professionals}} in {{Berkeley}}'s {{Museum}} of {{Vertebrate Zoology}}, 1907-39},
