@article{gobert_log_????,
  title = {From {{Log Files}} to {{Assessment Metrics}}: {{Measuring Students}}' {{Science Inquiry Skills Using Educational Data Mining}}},
  volume = {0},
  issn = {1050-8406},
  shorttitle = {From {{Log Files}} to {{Assessment Metrics}}},
  doi = {10.1080/10508406.2013.837391},
  abstract = {Abstract We present a method for assessing science inquiry performance, specifically for the inquiry skill designing and conducting experiments, using educational data mining on students' log data from online microworlds in the Inq-ITS system (http://www.inq-its.org). In our approach, we use a two-step process: First, we use text replay tagging, a type of rapid protocol analysis in which categories are developed and, in turn, used to hand score students' log data. In the second step, educational data mining is conducted using a combination of the text replay data and machine-distilled features of student interactions in order to produce an automated means of assessing the inquiry skill in question; this is referred to as a detector. Once this detector is appropriately validated, it can be applied to students' log files for auto-assessment and, in future, to drive scaffolding in real time. Furthermore, we present evidence that this detector developed in one scientific domain, Phase Change, can be used \textendash{} with no modification or re-training \textendash{} to effectively detect science inquiry skill in a second scientific domain, Density.},
