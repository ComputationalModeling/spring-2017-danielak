@article{novick_mathematical_1991,
  title = {Mathematical Problem Solving by Analogy.},
  volume = {17},
  issn = {0278-7393},
  doi = {10.1037/0278-7393.17.3.398},
  abstract = {Reports the results of 2 experiments and a verbal protocol study examining the component processes of solving mathematical word problems by analogy. Studies yielded 4 major findings. First, the process of mapping the features of the source and target problems and the process of adapting the source solution procedure for use in solving the target problem were clearly distinguished: (a) Successful mapping was found to be insufficient for successful transfer and (b) adaptation was found to be a major source of transfer difficulty. Second, we obtained direct evidence that schema induction is a natural consequence of analogical transfer. The schema was found to co-exist with the problems from which it was induced, and both the schema and the individual problems facilitated later transfer. Third, for our multiple-solution problems, the relation between analogical transfer and solution accuracy was mediated by the degree of time pressure exerted for the test problems. Finally, mathematical expertise was a significant predictor of analogical transfer, but general analogical reasoning ability was not. The implications of the results for models of analogical transfer and for instruction were considered. (PsycINFO Database Record (c) 2008 APA, all rights reserved)},
  timestamp = {2008-10-15T03:16:12Z},
  number = {3},
  urldate = {2008-10-15},
  journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
  author = {Novick, Laura R. and Holyoak, Keith J.},
