  year = {2011},
  keywords = {Humanities,Social Sciences and Law},
  pages = {359--383},
  file = {SpringerLink Full Text PDF:/Users/briandanielak/Documents/zotero/storage/H946MC6U/Hall - 2011 - Cultural Forms, Agency, and the Discovery of Inven.pdf:application/pdf;SpringerLink Snapshot:/Users/briandanielak/Documents/zotero/storage/G598DZXU/abstract.html:text/html},
