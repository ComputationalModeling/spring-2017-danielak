  keywords = {EDCI 770 - Foundations of Science Education,inquiry,Week 04},
  pages = {481--512},
  file = {Windschitl2004.pdf:/Users/briandanielak/Documents/zotero/storage/BPVJFU87/Windschitl2004.pdf:application/pdf},
  groups = {EDCI 770,EDCI 770}
}

