  journal = {ZDM},
  author = {Lee, Kerry and Yeong, Stephanie and Ng, Swee and Venkatraman, Vinod and Graham, Steven and Chee, Michael},
  year = {2010},
  keywords = {Humanities,Social Sciences and Law},
  pages = {591--605},
  file = {SpringerLink Snapshot:/Users/briandanielak/Documents/zotero/storage/8EX2K325/abstract.html:text/html},
