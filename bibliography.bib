  pages = {204--251},
  file = {Full Text PDF:/Users/briandanielak/Documents/zotero/storage/TJW6Z4VT/Wilkerson-Jerde and Wilensky - 2015 - Patterns, Probabilities, and People Making Sense .pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/H6BIGPKT/10508406.2014.html:text/html},
  groups = {Modeling}
}

