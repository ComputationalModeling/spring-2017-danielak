  file = {Full Text PDF:/Users/briandanielak/Documents/zotero/storage/ZAZV6GJ8/Fabrikant et al. - 2010 - Cognitively Inspired and Perceptually Salient Grap.pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/ZH2I85FF/00045600903362378.html:text/html}
