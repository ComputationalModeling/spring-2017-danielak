  file = {Full Text PDF:/Users/briandanielak/Documents/zotero/storage/IGN622M9/Wilkening and Fabrikant - 2013 - How users interact with a 3D geo-browser under tim.pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/8INFPFDG/15230406.2013.html:text/html}
}

