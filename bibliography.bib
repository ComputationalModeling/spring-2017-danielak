  booktitle = {Theories of {{Learning}} and {{Studies}} of {{Instructional Practice}}},
  publisher = {{Springer New York}},
  author = {Packer, Martin J.},
  editor = {Koschmann, Timothy and Spector, J. Michael and LaJoie, Susanne},
  year = {2011},
  keywords = {Humanities,Social Sciences and Law},
  pages = {167--188},
  file = {SpringerLink Full Text PDF:/Users/briandanielak/Documents/zotero/storage/6TNAN62B/Packer - 2011 - Schooling Domestication or Ontological Constructi.pdf:application/pdf;SpringerLink Snapshot:/Users/briandanielak/Documents/zotero/storage/8GB9MTQ9/abstract.html:text/html},
