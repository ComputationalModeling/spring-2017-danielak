@article{schelling_dynamic_1971,
  title = {Dynamic Models of Segregation},
  volume = {1},
  issn = {0022-250X},
  doi = {10.1080/0022250X.1971.9989794},
  abstract = {Some segregation results from the practices of organizations, some from specialized communication systems, some from correlation with a variable that is non-random; and some results from the interplay of individual choices. This is an abstract study of the interactive dynamics of discriminatory individual choices. One model is a simulation in which individual members of two recognizable groups distribute themselves in neighborhoods defined by reference to their own locations. A second model is analytic and deals with compartmented space. A final section applies the analytics to `neighborhood tipping.' The systemic effects are found to be overwhelming: there is no simple correspondence of individual incentive to collective results. Exaggerated separation and patterning result from the dynamics of movement. Inferences about individual motives can usually not be drawn from aggregate patterns. Some unexpected phenomena, like density and vacancy, are generated. A general theory of `tipping' begins to emerge.},
  timestamp = {2016-02-19T19:47:05Z},
  number = {2},
  urldate = {2016-02-19},
  journal = {The Journal of Mathematical Sociology},
  author = {Schelling, Thomas C.},
  month = jul,
  year = {1971},
  pages = {143--186},
  file = {Full Text PDF:/Users/briandanielak/Documents/zotero/storage/479KS57Q/Schelling - 1971 - Dynamic models of segregation.pdf:application/pdf;Schelling1971-OCR.pdf:/Users/briandanielak/Documents/zotero/storage/MAE9WJ3I/Schelling1971-OCR.pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/I7W3UK7H/0022250X.1971.html:text/html}
}

@article{clark_digital_2015,
  title = {Digital {{Games}}, {{Design}}, and {{Learning A Systematic Review}} and {{Meta}}-{{Analysis}}},
  issn = {0034-6543, 1935-1046},
  doi = {10.3102/0034654315582065},
  abstract = {In this meta-analysis, we systematically reviewed research on digital games and learning for K\textendash{}16 students. We synthesized comparisons of game versus nongame conditions (i.e., media comparisons) and comparisons of augmented games versus standard game designs (i.e., value-added comparisons). We used random-effects meta-regression models with robust variance estimates to summarize overall effects and explore potential moderator effects. Results from media comparisons indicated that digital games significantly enhanced student learning relative to nongame conditions ( = 0.33, 95\% confidence interval [0.19, 0.48], k = 57, n = 209). Results from value-added comparisons indicated significant learning benefits associated with augmented game designs ( = 0.34, 95\% confidence interval [0.17, 0.51], k = 20, n = 40). Moderator analyses demonstrated that effects varied across various game mechanics characteristics, visual and narrative characteristics, and research quality characteristics. Taken together, the results highlight the affordances of games for learning as well as the key role of design beyond medium.},
  language = {en},
  timestamp = {2016-02-29T22:21:49Z},
  urldate = {2016-02-29},
  journal = {Review of Educational Research},
  author = {Clark, Douglas B. and Tanner-Smith, Emily E. and Killingsworth, Stephen S.},
  month = apr,
  year = {2015},
  keywords = {digital games,Learning,meta-analysis,systematic review},
  pages = {0034654315582065},
  file = {Full Text PDF:/Users/briandanielak/Documents/zotero/storage/TMZM4WP2/Clark et al. - 2015 - Digital Games, Design, and Learning A Systematic R.pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/GT75NPPD/0034654315582065.html:text/html}
}

@misc{_thawing_????,
  title = {Thawing from a {{Long Winter}} in {{Computer Science Education}}},
  abstract = {While the market need to teach computer science skills is undeniable and while equity demands giving all students the opportunity to learn them, operationalizing this kind of education for all learners requires careful planning and execution.  Under the leadership of Jan Cuny and others, the National Science Foundation has focused on curricula in secondary schools, including Advanced Placement (AP) and pre-Advanced Placement classes, as well as incorporating computer science in K-12 science classes and researching assessment in the field.  Young students in grades K-6 should learn the basic ideas in computing and how to solve problems computationally.  Students in grades 6-8 should progress to more complex computer science concepts and should learn about the people who have made computing possible to pique career interests.},
