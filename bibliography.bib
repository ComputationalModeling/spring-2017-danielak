  file = {ACM Full Text PDF:/Users/briandanielak/Documents/zotero/storage/QVU6AESU/Bederson et al. - 2002 - Ordered and quantum treemaps Making effective use.pdf:application/pdf;ACM Snapshot:/Users/briandanielak/Documents/zotero/storage/ATCF3FK5/citation.html:text/html},
  groups = {CMSC 734}
}

