@phdthesis{stevens_disciplined_1999,
  address = {United States -- California},
  title = {Disciplined Perception: {{Comparing}} the Development of Embodied Mathematical Practices in School and at Work},
  shorttitle = {Disciplined Perception},
  abstract = {This dissertation compares the working and learning practices in a middle-school mathematics classroom and a professional architecture firm. The comparative analysis is based upon ethnographic case studies of each setting. The primary data are representations of transcribed talk and embodied action assembled from a large corpus of audio-video recordings made in each setting. Both case studies are instances of socio-genetic analysis , documenting the development of teams of people working together on architectural design projects under very different organizational conditions. Focal attention in each case is given to how mathematical practices are and become practical resources for people. Drawing on cognitive, pragmatist, and interactionist research traditions, the core chapters develop an empirically grounded interpretative framework for understanding how cognition and learning occur "in the wild". Across the case studies, I argue that contexts for mathematical activity vary dynamically and routinely across local spaces, participant structures, and situations. In Chapter Two my case study of the classroom identifies two distinct contexts for mathematical activity, one corresponding to traditional school mathematics practices (the official mode) and another corresponding to mathematical practices that arise in design project work (the emergent mode). Through a comparison a team's activities in each mode, I seek to show how mathematics, individual competence, and collaboration appear distinctly different in the two modes. I show how the oscillation between these modes posed dilemmas for the teacher and students alike, and argue that this bimodality is general characteristic of contemporary project-based classrooms operating within the traditional organizational environment of a school. In Chapter Three I explore the mathematical activities involved in professional architectural project work, making a primary distinction between mathematical activities that were embodied in architects' routine tool-mediated work and mathematical activities that were distributed to other participants in project work, such as engineers. In general, I show that mathematical activities, while being ubiquitous in architectural work, were enacted in unfamiliar forms and served unfamiliar functions (from the perspective of what usually "counts as" mathematics) in architectural practice. In Chapter Four I offer an interpretative framework for understanding and comparing the cognitive activities of the students and the architects at an interactional level. This framework-- disciplined perception --treats disciplinary knowledge in terms of what people do across the coordinated modalities of talk, gesture, and inscription. This framework is offered as an alternative to traditional mentalistic conceptions of knowledge. My comparative analyses identify similarities and differences in the form, content, and function of disciplined perception across the cases. In Chapter Five I compare the classroom and the architecture firm from an organizational perspective, arguing that the classroom was a comparatively lousy place to learn and work for the students. Reasons for this comparative deficit include: an inadequate infrastructure for project work, limited access to consequential interactional resources, and an assessment system from which students could learn very little. The comparison with the organizational environment of the architecture firm is therefore a benchmark for exploring the conditions that make learning and meaningful work possible. In the concluding chapter I describe how these comparative case studies provide a new perspective on issues of cognition and development as well as furnishing productive resources for innovators seeking to improve discipline-based education.},
  timestamp = {2012-10-11T15:39:23Z},
  school = {University of California, Berkeley},
  author = {Stevens, Reed},
  year = {1999},
  note = {Ph.D.},
  keywords = {Architecture,Educational psychology,Educational sociology,Mathematics Education,Secondary education},
  file = {StevensDissertation1999OCR.pdf:/Users/briandanielak/Documents/zotero/storage/8G3I7GR4/StevensDissertation1999OCR.pdf:application/pdf;Document View - ProQuest:/Users/briandanielak/Documents/zotero/storage/MQPCTDRI/pqdweb.html:text/html}
}

@article{bereiter_principled_????,
  title = {Principled {{Practical Knowledge}}: {{Not}} a {{Bridge}} but a {{Ladder}}},
  volume = {0},
  issn = {1050-8406},
  shorttitle = {Principled {{Practical Knowledge}}},
  doi = {10.1080/10508406.2013.812533},
  abstract = {The much-lamented gap between theory and practice in education cannot be filled by practical knowledge alone or by explanatory knowledge alone. Principled practical knowledge (PPK) is a type of knowledge that has characteristics of both practical know-how and scientific theory. Like basic scientific theory, PPK meets standards of explanatory coherence. However, its main function is not explanation or prediction but practical guidance. PPK grows out of efforts to solve practical problems, but it requires additional effort invested in producing knowledge that goes beyond what is required for the task at hand yet not so far beyond as to be unusable by practitioners. The Wright brothers' construction of PPK to address problems of flight control is used as a model for building such knowledge in the learning sciences. Design-based research in the learning sciences may motivate research into basic theoretical questions, but it is unlikely to contribute directly to answering them. Extending design-based research to the creation of PPK can, however, increase the generalizability of knowledge produced through design work and provide a ladder leading to sometimes radical design improvement.},
  timestamp = {2013-10-26T05:31:19Z},
  number = {0},
  urldate = {2013-10-26},
  journal = {Journal of the Learning Sciences},
  author = {Bereiter, Carl},
  keywords = {DBR - design based research,Pragmatism},
  pages = {1--14},
