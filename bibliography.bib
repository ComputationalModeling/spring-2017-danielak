@article{reisslein_pre-college_2010,
  title = {Pre-College {{Electrical Engineering Instruction}}: {{The Impact}} of {{Abstract}} vs. {{Contextualized Representation}} and {{Practice}} on {{Learning}}},
  volume = {99},
  copyright = {2010 American Society for Engineering Education},
  issn = {2168-9830},
  shorttitle = {Pre-College {{Electrical Engineering Instruction}}},
  doi = {10.1002/j.2168-9830.2010.tb01058.x},
  abstract = {BackgroundWhile engineering instructional materials and practice problems for pre-college students are often presented in the context of real-life situations, college-level texts are typically written in abstract form.Purpose (Hypothesis)The goal of this study was to jointly examine the impact of contextualizing engineering instruction and varying the number of practice opportunities on pre-college students' learning and learning perceptions.Design/MethodUsing a 3 \texttimes{} 2 factorial design, students were randomly assigned to learn about electrical circuit analysis with an instructional program that represented problems in abstract, contextualized, or both forms, either with two practice problems or four practice problems. The abstract problems were devoid of any real-life context and represented with standard abstract electrical circuit diagrams. The contextualized problems were anchored around real-life scenarios and represented with life-like images. The combined contextualized-abstract condition added abstract circuit diagrams to the contextualized representation. To measure learning, students were given a problem-solving near-transfer post-test. Learning perceptions were measured using a program-rating survey where students had to rate the instructional program's diagrams, helpfulness, and difficulty.ResultsStudents in the combined contextualized-abstract condition scored higher on the post-test, produced better problem representations, and rated the program's diagrams and helpfulness higher than their counterparts. Students who were given two practice problems gave higher program diagram and helpfulness ratings than those given four practice problems.ConclusionsThese findings suggest that pre-college engineering instruction should consider anchoring learning in real-life contexts and providing students with abstract problem representations that can be transferred to a variety of problems.},
  language = {en},
  timestamp = {2013-07-08T20:36:54Z},
  number = {3},
  urldate = {2013-07-08},
  journal = {Journal of Engineering Education},
  author = {Reisslein, Martin and Moreno, Roxana and Ozogul, Gamze},
  year = {2010},
  keywords = {practice,problem representation,Problem Solving},
  pages = {225--235},
  file = {Snapshot:/Users/briandanielak/Documents/zotero/storage/XV7NVEJ3/abstract\;jsessionid=A35ACAB192869EDC5373BAF49A6CDE4B.html:text/html}
}

@article{berland_using_2013,
  title = {Using {{Learning Analytics}} to {{Understand}} the {{Learning Pathways}} of {{Novice Programmers}}},
  volume = {22},
  issn = {1050-8406},
  doi = {10.1080/10508406.2013.836655},
  abstract = {Abstract Nearly every emerging profession in the US requires some ability to program (Wing, 2006). However, few students learn any programming (Klein, Cavanagh, Kay, \& Meisinger, 2006). This gap in learning leaves students woefully under-prepared for many of the most rapidly expanding job fields, and the students most likely to be affected by this illiteracy are those who are already most significantly underrepresented in the STEM pipeline (Margolis, Estrella, Goode, Holme, \& Nao, 2008). As a research field, there is much work left to do in understanding how novices learn to program (Pears, Daniels, \& Berglund, 2002); little is known about the optimal pathways for beginners. Many have suggested that tinkering plays a critical role (e.g. Hancock, 2001), and more fine-grained methods could provide more robust descriptions of the relationship between tinkering and learning. Recently, new ways to better understand learning mechanisms have come from learning analytics (Blikstein, 2011) and educational data mining (Baker \& Yacef, 2009). This work uses these methods to show that students learned through a pathway that we term EXTIRE (for exploration, tinkering, and refinement). Our use of these methods has also allowed us to contribute to learning sciences by: showing empirical support for previously theorized processes; enabling us to describe and identify a role of tinkering in novices' learning; and presenting a new approach to creating process descriptions similar to those more typically found in qualitative research. Furthermore, our findings illuminate how tinkering can serve as a valuable approach for novices.},
  timestamp = {2014-12-08T20:27:52Z},
  number = {4},
  urldate = {2013-10-26},
  journal = {Journal of the Learning Sciences},
  author = {Berland, Matthew and Martin, Taylor and Benton, Tom and Petrick Smith, Carmen and Davis, Don},
  year = {2013},
  pages = {564--599},
  file = {BerlandEtAl.pdf:/Users/briandanielak/Documents/zotero/storage/FMSJNRXP/BerlandEtAl.pdf:application/pdf;Snapshot:/Users/briandanielak/Documents/zotero/storage/PRITDPD6/10508406.2013.html:text/html}
}

