  keywords = {ChemEd Research,EDCI 780 Literature Review,Knowledge Level,Mathematical Ability,Models,problem features; transfer during solution of complex arithmetic problems; expert vs novice college students; implications for models of analogical problem solving,Problem Solving,Transfer (Learning)},
  pages = {510--520},
  file = {Novick1988.pdf:/Users/briandanielak/Documents/zotero/storage/9UFPEBZ6/Novick1988.pdf:application/pdf},
  groups = {ChemEd Research,EDCI 780 Lit Review,ChemEd Research,EDCI 780 Lit Review,EDCI 780 Lit Review}
