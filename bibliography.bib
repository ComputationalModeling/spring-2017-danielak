  author = {Strijbos, Jan-Willem and Martens, Rob L. and Prins, Frans J. and Jochems, Wim M.G.},
  month = jan,
  year = {2006},
  keywords = {Coding,Content analysis,Methodology,RELIABILITY,Segmentation,Unit of analysis},
  pages = {29--48},
